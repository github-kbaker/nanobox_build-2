# Production Environment Variables for Railway
# These will be set in Railway's dashboard, this file is just for reference

# MongoDB connection (Railway will provide this)
MONGO_URL="mongodb://mongo:27017/production_db"

# Database name
DB_NAME="production_database"

# FastAPI configuration
ENVIRONMENT="production"
DEBUG=False

# CORS Origins (update with your frontend domain)
ALLOWED_ORIGINS=["https://your-frontend-domain.com", "https://localhost:3000"]